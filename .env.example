# Copy this file to .env and fill in your values. Never commit .env.

# Stripe (get from Stripe Dashboard → Developers → API keys)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_publishable_key_here
STRIPE_SECRET_KEY=sk_live_your_secret_key_here

# Supabase
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=eyJxxx

# Optional: webhook and fees account (for stripe-webhook Edge Function)
# STRIPE_WEBHOOK_SECRET=whsec_xxx
# STRIPE_FEES_ACCOUNT_ID=acct_xxx

# Optional: invite site
# EXPO_PUBLIC_INVITE_WEB_BASE_URL=https://yoursite.vercel.app
